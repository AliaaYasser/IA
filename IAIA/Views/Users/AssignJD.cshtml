
@model IEnumerable<IAIA.Models.User>
@{
    ViewBag.Title = "AssignJD";
}

<h2>AssignJD</h2>



<p>
    @using (Html.BeginForm("AssignJD", "Users", FormMethod.Get, new { pid = ViewBag.pid, pmid = ViewBag.pmid }))
    {
        <input type="hidden" name="pmid" value="@ViewBag.pmid" />
        <input type="hidden" name="pid" value="@ViewBag.pid" />
        @Html.TextBox("search") <input type="submit" value="Search" />

    }
</p>

<div class="gallery">
    <div class="gallery-bottom grid">



        @foreach (var item in Model)
        {
        <div class="col-md-3 g-left">
            <a href='@Url.Action("Details", "Users",new { id=item.UserId})' rel="title" class="b-link-stripe b-animate-go  thickbox">
                <figure class="effect-oscar">

                    <img src="@Url.Content(item.UserPhoto)" />
                    <figcaption>
                        <h4>@item.UserFirstName</h4>
                        <p><i class="lnr lnr-magnifier"></i></p>
                    </figcaption>
                </figure>
            </a>
            <br />
            @Html.ActionLink("send Requst", "SendRequst", new { pid = ViewBag.pid, pmid = ViewBag.pmid, toid = item.UserId })
        </div>
        }


    </div>
</div>
              

   


